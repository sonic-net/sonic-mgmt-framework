{{>partial_header}}
package {{packageName}}

{{#operations}}
import (
	"net/http"

	"github.com/Azure/sonic-mgmt-framework/rest/server"
){{#operation}}

func {{nickname}}(w http.ResponseWriter, r *http.Request) {
    rc, r := server.GetContext(r)
    rc.Name = "{{operationId}}"
    {{#consumes}}
    rc.Consumes.Add("{{mediaType}}")
    {{/consumes}}
    {{#produces}}
    rc.Produces.Add("{{mediaType}}")
    {{/produces}}
	server.Process(w, r)
}{{/operation}}{{/operations}}
