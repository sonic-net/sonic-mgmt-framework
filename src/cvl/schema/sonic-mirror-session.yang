module sonic-mirror-session {
	namespace "http://github.com/Azure/sonic-mirror-session";
	prefix sms;

	import ietf-yang-types {
		prefix yang;
	}

	import ietf-inet-types {
		prefix inet;
	}

	import sonic-common {
		prefix scommon;
	}

	organization
		"SONiC";

	contact
		"SONiC";

	description
		"SONIC MIRROR SESSION";

	revision 2019-05-15 {
		description
			"Initial revision.";
	}

	container sonic-mirror-session {

		list MIRROR_SESSION {
			key "name";

			leaf name {
				type string;
			}

			leaf src_ip {
				type inet:ipv4-address;
			}

			leaf dst_ip {
				type inet:ipv4-address;
			}

			leaf gre_type {
				type string;
			}

			leaf dscp {
				type uint8;
			}

			leaf ttl {
				type uint8;
			}

			leaf queue {
				type uint8;
			}
		}
	}
}
